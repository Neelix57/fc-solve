{% macro js_requirejs(production) %}
{% if production %}
<script src="{{ base_path }}js/require.js"></script>
{% else %}
<script src="{{ base_path }}js/require--debug.js"></script>
{% endif %}
{% endmacro %}
{% macro jquery_ui_head_elements(enable_jquery_ui) %}
{% if enable_jquery_ui %}
<script src="{{ base_path }}js/jquery-ui/jquery-ui.min.js"></script>
<link rel="stylesheet" href="{{ base_path }}js/jquery-ui/jquery-ui.min.css" media="screen" />
<link rel="stylesheet" href="{{ base_path }}jqui-override.css" media="screen" />
{% endif %}
{% endmacro %}
