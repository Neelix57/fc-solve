{% extends "template.jinja" %}
{% block title %}Online Freecell solver (Automated tests){% endblock %}


{% block page_extra_head_elements %}
<script src="{{ base_path }}js/jquery.querystring.js"></script>
{{ jquery_ui_head_elements() }}
<script src="{{ base_path }}js/libfreecell-solver.min.js"></script>
<script src="{{ base_path }}js/web-fc-solve--expand-moves.js"></script>
<script src="{{ base_path }}js/web-fc-solve.js"></script>
<link rel="stylesheet" type="text/css" href="{{ base_path }}web-fc-solve.css" />
<script src="{{ base_path }}js/jquery.ajaxQueue.min.js"></script>
<script src="{{ base_path }}js/web-fc-solve--expand-moves--mega-test.js"></script>
{% endblock %}

{% block body %}
<h2 id="about">About</h2>

<h2 id="deal_idx_update" style="color: red">Not started</h2>

<script>
  $(document).ready(function () {
      try {
          test_js_fc_solve_class();
      }
      catch (err) {
          alert(err);
      }
  });
</script>

<h2>AJAX Output</h2>

<textarea id="ajax_out"></textarea>

<h2>JS-FCS Output</h2>

<textarea id="js_fcs_out"></textarea>

<h1 id="qunit-header">QUnit example</h1>
<h2 id="qunit-banner">Banner</h2>
<div id="qunit-testrunner-toolbar"></div>
<h2 id="qunit-userAgent">User Agent</h2>
<ol id="qunit-tests"></ol>
<div id="qunit-fixture">test markup, will be hidden</div>

{% endblock %}
