#!/bin/bash
rebuild()
{
    (
        set -x
        set -e
        time git clean -dxf .
        time bash -x bin/install-npm-deps.sh
        time ./gen-helpers
        time gmake serial_run
        time gmake -j8 fastrender
        time gmake -j8 PROD=1 fastrender
        time gmake -j8 PROD=1 all
        time gmake -j8 test
    )
}
rebuild
